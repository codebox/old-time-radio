{
  "name": "old-time-radio",
  "version": "1.0.0",
  "description": "A website for listening to Old Time Radio hosted by archive.org",
  "main": "src_old/app.js",
  "scripts": {
    "start": "node src/app.mjs",
    "clean:shared": "rm -f shared/*.mjs shared/*.mjs.map shared/*.d.mts shared/tsconfig.tsbuildinfo",
    "clean:backend": "rm -f src/*.mjs.map src/tsconfig.tsbuildinfo",
    "clean:frontend": "rm -f public/js/*.mjs public/js/*.mjs.map public/js/tsconfig.tsbuildinfo",
    "clean": "npm run clean:shared && npm run clean:backend && npm run clean:frontend",
    "build:shared": "tsc --build shared/tsconfig.json",
    "build:backend": "tsc -p src/tsconfig.json --noEmit",
    "build:frontend": "tsc --build public/js/tsconfig.json",
    "build": "npm run build:shared && npm run build:backend && npm run build:frontend",
    "rebuild": "npm run clean && npm run build",
    "typecheck": "tsc --build --noEmit shared/tsconfig.json src/tsconfig.json public/js/tsconfig.json",
    "watch:frontend": "tsc --build --watch public/js/tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codebox/old-time-radio.git"
  },
  "author": "Rob Dawson <rob@codebox.net> (http://codebox.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codebox/old-time-radio/issues"
  },
  "homepage": "https://github.com/codebox/old-time-radio/issues",
  "dependencies": {
    "axios": "^1.6.0",
    "ejs": "^3.1.10",
    "express": "^5.2.1",
    "jasmine": "^3.7.0",
    "level": "^9.0.0",
    "lru-cache": "^11.1.0",
    "proxyquire": "^2.1.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/express": "^5.0.6",
    "@types/node": "^22.13.14",
    "@types/winston": "^2.4.4",
    "typescript": "^5.7.3"
  }
}
